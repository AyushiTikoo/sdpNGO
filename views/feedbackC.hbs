        <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
        <style>
            body{
                position: relative; 
                font-family: cursive;
            }
            .stars-outer {
            position: relative;
            display: inline-block;
            }

            .stars-inner {
            position: absolute;
            top: 0;
            left: 0;
            white-space: nowrap;
            overflow: hidden;
            width: 0;
            }

            .stars-outer::before {
            content: "\f005 \f005 \f005 \f005 \f005";
            font-family: "Font Awesome 5 Free";
            font-weight: 900;
            color: #ccc;
            }

            .stars-inner::before {
            content: "\f005 \f005 \f005 \f005 \f005";
            font-family: "Font Awesome 5 Free";
            font-weight: 900;
            color: #f8ce0b;
            }
        </style>
    </head> 
    <br>
        <a href="dashboard" class="btn btn-dark" style="margin-left:50px;background-color:black;width:100px;font-family:cursive;">Go Back</a>
    <br>                     
        <div class="container mt-5">
            <form action="/feedbackC/submit" method="post">
                <div class="form-group">
                    <label for="email">Email:</label>
                    <input type="email" placeholder="Email"  class="form-control" id="email" name="email"
                    autofocus required autocomplete="off" >    
                </div>
                
                <div class="form-group">
                    <label for="category">Select Category:</label>
                    <select id="category" class="form-control " name="category">
                        <option value="0" disabled selected>Select Category</option>
                        <option value="Education">Education</option>
                        <option value="Health">Health</option>
                        <option value="Livelihood">Livelihood</option>
                        <option value="Environment">Environment</option>
                        <option value="Child Protection">Child Protection</option>
                    </select>
                </div>
                <div class="form-group">
                  <label for="comments">Comments:</label>
                  <input type="text" placeholder="Comments"  class="form-control" id="comments" name="comments"
                  autofocus required autocomplete="off" >    
              </div>
                <button type="submit" style=" margin-left:400px;  margin-top: 30px; width: 250px; margin-bottom: 30px;"
                class="btn btn-warning">Submit Feedback</button>
            </form>
        </div>